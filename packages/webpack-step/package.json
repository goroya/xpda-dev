{
  "name": "@xpda-dev/webpack-step",
  "version": "0.1.0",
  "description": "Cross platform desktop app development tools",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "ava",
    "test:coverage": "nyc ava"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michalzaq12/xpda-dev.git"
  },
  "keywords": [],
  "author": "“Michał <“michal.m.zarach@gmail.com”>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michalzaq12/xpda-dev/issues"
  },
  "homepage": "https://github.com/michalzaq12/xpda-dev#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "webpack": "*",
    "ts-loader": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@xpda-dev/core": "^0.1.0",
    "webpack": "^4.39.1",
    "ts-loader": "^6.0.4"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/src/**/*.spec.ts"
    ],
    "snapshotDir": "test/snapshots"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "src/**/*.ts"
    ],
    "all": true
  }
}
